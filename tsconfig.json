{
	"compilerOptions": {
		"target": "es2020",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "react",
		"declaration": true,
		"outDir": "./dist",
		"types": ["node", "express"]
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules"]
}
